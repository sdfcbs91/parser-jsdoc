{
	"publisher": "sdfcbs",
	"name": "jsdoc",
	"displayName": "vscode-jsdoc",
	"description": "add jsdoc template",
	"version": "0.0.1",
	"repository": {
		"url": "",
		"type": "git"
	},
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Extension Packs"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.addJSDoc",
				"title": "add jsdoc",
				"category": "jsdoc"
			}
		],
		"menus": {
			"commandPalette": []
		},
		"configuration": [
			{
				"title": "jsdoc",
				"properties": {
					"jsdoc.author": {
						"type": "string",
						"default": "",
						"description": "Add this option, jsdoc will automatically insert the author 添加此选项, jsdoc 将自动插入作者"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"package": "vsce package"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.48",
		"@types/node": "^8.10.66",
		"typescript": "^3.9.10",
		"vscode": "^1.1.37"
	}
}
