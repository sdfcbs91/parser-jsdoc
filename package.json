{
	"publisher": "jsdoc",
	"name": "jsdoc",
	"displayName": "jsdoc",
	"description": "gen jsdoc template",
	"version": "1.0.2",
	"repository": {
		"url": "https://github.com/lllllllqw/vscode-jsdoc.git",
		"type": "git"
	},
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Extension Packs"
	],
	"activationEvents": [
		"*"
	],
	"main": "./src/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.addJSDoc",
				"title": "add jsdoc"
			}
		],
		"configuration": [
			{
				"title": "jsdoc",
				"properties": {
					"jsdoc2.author": {
						"type": "string",
						"default": "",
						"description": "添加此选项, jsdoc 将自动插入作者"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"package": "vsce package"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
